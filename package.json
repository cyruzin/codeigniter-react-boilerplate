{
  "name": "codeigniter-react-boilerplate",
  "version": "1.2.2",
  "description": "A CodeIgniter boilerplate with React.",
  "main": "src/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "webpack --mode development --watch --progress --config config/webpack.dev.js",
    "build": "webpack --mode production --config config/webpack.prod.js",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyruzin/codeigniter-react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "codeigniter",
    "php",
    "js",
    "react",
    "webpack",
    "eslint",
    "prettier"
  ],
  "author": "Cyro Dubeux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyruzin/codeigniter-react-boilerplate/issues"
  },
  "homepage": "https://github.com/cyruzin/codeigniter-react-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.4.6",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
