{
  "name": "codeigniter-react-boilerplate",
  "version": "1.2.1",
  "description": "A CodeIgniter boilerplate with React.",
  "main": "src/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "webpack --mode development --watch --progress --config config/webpack.dev.js",
    "build": "webpack --mode production --config config/webpack.prod.js",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyruzin/codeigniter-react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "codeigniter",
    "php",
    "js",
    "react",
    "webpack",
    "eslint",
    "prettier"
  ],
  "author": "Cyro Dubeux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyruzin/codeigniter-react-boilerplate/issues"
  },
  "homepage": "https://github.com/cyruzin/codeigniter-react-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "mini-css-extract-plugin": "^1.3.3",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.12.1",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
